<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>ustructexported</Title>
			<Shortcut>ustructexported</Shortcut>
			<Description>插件和模块的 Unreal Engine USTRUCT 宏</Description>
			<Author>Microsoft Corporation</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<Default>MyStruct</Default>
					<ToolTip>USTRUCT 名称</ToolTip>
				</Literal>
				<Literal>
					<ID>apiname</ID>
					<Default>MYPROJECT</Default>
					<ToolTip>USTRUCT API 名称</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="cpp"><![CDATA[USTRUCT()
				struct $apiname$_API F$name$
				{
				GENERATED_BODY()
				$end$
				};]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
