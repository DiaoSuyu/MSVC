    <xs:element name="WarningsAsErrors" type="msb:StringPropertyType" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="WarningsAsErrors" _locComment="" -->以逗号分隔的警告编号的列表，要将这些警告视为错误</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="WebPage" type="msb:StringPropertyType" substitutionGroup="msb:Property"/>
    <xs:element name="Win32ResourceFile" type="msb:StringPropertyType" substitutionGroup="msb:Property"/>
    <xs:element name="WholeProgramOptimization" type="msb:StringPropertyType" substitutionGroup="msb:Property"/>
    <xs:element name="TieredCompilation" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="TieredCompilation" _locComment="" -->指示运行时是否应启用分层 JIT 编译。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="ServerGarbageCollection" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="ServerGarbageCollection" _locComment="" -->指示运行时是否应启用服务器垃圾回收模式。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="UserSecretsId" type="msb:StringPropertyType" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="UserSecretsId" _locComment="" -->将用于在开发时查找存储此项目的机密配置值的文件的 ID。</xs:documentation>
        </xs:annotation>
    </xs:element>

    <!-- ======================== RAZOR PROPERTIES ======================== -->
    <xs:element name="RazorCompileOnBuild" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="RazorCompileOnBuild" _locComment="" -->指示是否应在生成时编译 Razor 文件。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="RazorCompileOnPublish" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="RazorCompileOnPublish" _locComment="" -->指示是否应在发布时编译 Razor 文件。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="UseRazorBuildServer" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="UseRazorBuildServer" _locComment="" -->指示 Razor 代码生成是否应使用持久性生成服务器进程。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="RazorOutputPath" type="msb:StringPropertyType" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="RazorOutputPath" _locComment="" -->Razor 输出的目录。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="EnableDefaultRazorGenerateItems" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="EnableDefaultRazorGenerateItems" _locComment="" -->设置为 true 可自动在 @(Content) 的 @(RazorGenerate) 中包含 Razor (.cshtml) 文件。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="EnableDefaultContentItems" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="EnableDefaultContentItems" _locComment="" -->设置为 true 可自动将某些文件类型(如 .cshtml 文件)作为内容添加到项目中。当通过 Microsoft.NET.Sdk.Web 引用时，这还包括 wwwroot 下的所有文件和任何配置文件。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="EnableDefaultRazorComponentItems" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="EnableDefaultRazorComponentItems" _locComment="" -->设置为 true 可自动在 @(Content) 的 @(RazorComponent) 中包含 Razor (.razor) 文件。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="CopyRazorGenerateFilesToPublishDirectory" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="CopyRazorGenerateFilesToPublishDirectory" _locComment="" -->设置为 true 可将 RazorGenerate 项 (.cshtml) 复制到发布目录。如果发布的应用程序在生成时或发布时参与编译，则通常不需要 Razor 文件。默认情况下，Razor SDK 将禁止将 RazorGenerate 项复制到发布目录。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="CopyRefAssembliesToPublishDirectory" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="CopyRefAssembliesToPublishDirectory" _locComment="" -->设置为 true 可将引用程序集项复制到发布目录。如果在生成时或发布时进行 Razor 编译，则发布的应用程序不需要使用引用程序集。默认情况下，Razor SDK 将禁止将引用程序集复制到发布目录。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="IncludeRazorContentInPack" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="IncludeRazorContentInPack" _locComment="" -->配置是否将所有 Razor 内容项(cshtml 文件)都标记为内容包含在生成的 NuGet 包中。所有内容项都作为内容文件包含在 NuGet 包中。此设置可用于控制 Razor 内容项的此行为。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="RazorTargetName" type="msb:StringPropertyType" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="RazorTargetName" _locComment="" -->Razor 生成的程序集的文件名(不包括扩展名)。</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="EmbedRazorGenerateSources" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="EmbedRazorGenerateSources" _locComment="" -->配置是否将 @(RazorGenerate) 项作为嵌入文件添加到生成的程序集。如果为 true，@(RazorGenerate) 中的所有内容都将添加到 @(RazorEmbeddedFiles) 并传递给 CSC。</xs:documentation>
        </xs:annotation>
    </xs:element>

    <!-- ======================== gRPC properties ======================== -->
    <xs:element name="DisableProtobufDesignTimeBuild" type="msb:boolean" substitutionGroup="msb:Property">
        <xs:annotation>
            <xs:documentation><!-- _locID_text="DisableProtobufDesignTimeBuild" _locComment="" -->指示是否应为引用的 @(Protobuf) 文件禁用设计时生成。</xs:documentation>
        </xs:annotation>
    </xs:element>

    <!-- ======================== DOTNET PACK PROPERTIES =====================================-->
    <xs:element name="AssemblyTitle" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AssemblyTitle" _locComment="" -->程序集清单的说明</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="AssemblyVersion" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AssemblyVersion" _locComment="" -->major.minor.patch 形式的程序集清单版本数值(如 2.4.0)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Authors" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Copyright" _locComment="" -->以逗号分隔的 NuGet 包作者列表</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Company" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Company" _locComment="" -->程序集清单的公司名</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Copyright" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Copyright" _locComment="" -->NuGet 包的版权详细信息</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Description" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Description" _locComment="" -->UI 显示的 NuGet 包的长说明</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="FileVersion" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="FileVersion" _locComment="" -->major.minor.build.revision 形式的程序集清单版本数值(如 2.4.0.1)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="InformationalVersion" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="InformationalVersion" _locComment="" -->UI 显示的程序集产品版本(如 1.0 Beta)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="NeutralLanguage" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="NeutralLanguage" _locComment="" -->NuGet 包的区域设置 ID</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageIconUrl" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageIconUrl" _locComment="" -->带透明背景的用作 UI 中 NuGet 包图标的 64x64 图像的 URL</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageId" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_textPackageId _locComment="" -->The case-insensitive NuGet package identifier, which must be unique across nuget.org or whatever gallery the NuGet package will reside in. IDs may not contain spaces or characters that are not valid for a URL, and generally follow .NET namespace rules.</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Title" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_textTitle _locComment="" -->A human-friendly title of the package, typically used in UI displays as on nuget.org and the Package Manager in Visual Studio. If not specified, the package ID is used instead.</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageLicenseUrl" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageLicenseUrl" _locComment="" -->NuGet 包许可证的 URL，通常显示在 UI 和 nuget.org 中</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageProjectUrl" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageProjectUrl" _locComment="" -->NuGet 包主页的 URL，通常显示在 UI 和 nuget.org 中</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageReleaseNotes" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageReleaseNotes" _locComment="" -->此版本 NuGet 包中所作更改的说明，通常代替包说明用在 UI 中，如 Visual Studio 包管理器的“更新”选项卡</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageRequireLicenseAcceptance" type="msb:boolean" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageRequireLicenseAcceptance" _locComment="" -->一个指示客户端是否必须提示使用者在安装包前接受 NuGet 包许可证的值</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageTags" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageTags" _locComment="" -->以空格分隔的标记和关键字列表，描述 NuGet 包并通过搜索和筛选机制辅助 NuGet 包的可发现性</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageVersion" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageVersion" _locComment="" -->major.minor.patch 形式的 NuGet 包版本数值(如 1.0.1)。版本号可能包含预发布版本后缀(如 1.0.1-beta)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Product" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Product" _locComment="" -->程序集清单的产品名信息</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="Version" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="Version" _locComment="" -->major.minor.patch 形式的版本数值(如 2.4.0)</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="VersionPrefix" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="VersionPrefix" _locComment="" -->未指定版本时，VersionPrefix 表示版本字符串的第一个片段(如 1.0.0)。语法为 VersionPrefix[-VersionSuffix]。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="VersionSuffix" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="VersionSuffix" _locComment="" -->未指定版本时，VersionSuffix 表示版本字符串的第二个片段(如 beta)。语法为 VersionPrefix[-VersionSuffix]。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="RestoreSources" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="RestoreSources" _locComment="" -->NuGet 将用于还原此项目的包的源。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageLicenseExpression" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageLicenseExpression" _locComment="" -->项目许可证的 SPDX 标识符。仅 OSI 和 FSF 已批准的许可证可以使用标识符。其他许可证应使用 PackageLicenseFile。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageLicenseFile" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageLicenseFile" _locComment="" -->包的许可证文件的路径。仅当包不使用 OSI 或 FSF 批准的许可证时才应使用。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PackageType" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PackageType" _locComment="" -->指示预期的包用途(例如 .NET CLI 全局工具、标准依赖项等)。</xs:documentation>
      </xs:annotation>
    </xs:element>

    <!-- ======================== Publish properties =====================================-->

    <xs:element name="PublishReadyToRun" type="msb:boolean" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PublishReadyToRun" _locComment="" -->指示项目是否应在发布期间生成 ReadyToRun 映像。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PublishAot" type="msb:boolean" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PublishAot" _locComment="" -->指示项目是否应在发布期间生成本机提前编译的映像。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PublishSingleFile" type="msb:boolean" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PublishSingleFile" _locComment="" -->指示项目是否应在发布期间将所有依赖应用程序的文件捆绑到单个二进制文件中。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="PublishTrimmed" type="msb:boolean" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="PublishTrimmed" _locComment="" -->指示项目是否应在发布过程中生成裁切的程序集映像。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="TrimmerRootAssembly" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="TrimmerRootAssembly" _locComment="" -->不应剪裁的程序集(指定不带扩展名的程序集名称)。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="TrimmerRootDescriptor" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="TrimmerRootDescriptor" _locComment="" -->XML 文件，用于指定不应剪裁的程序集、类型及其成员。</xs:documentation>
      </xs:annotation>
    </xs:element>

    <!-- ======================== XAMARIN.ANDROID PROPERTIES ======================== -->

    <xs:element name="AndroidApplication" type="msb:boolean" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AndroidApplication" _locComment="" -->由 Xamarin.Android 项目使用。一个布尔值，指示相关项目是用于 Android 应用程序(True)还是 Android 库项目(False 或不存在)。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="AndroidDexTool" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AndroidDexTool" _locComment="" -->由 Xamarin.Android 项目使用。一个字符串属性，指示在 Xamarin.Android 生成过程中使用哪种 Android dex 编译器。</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="d8"/>
          <xs:enumeration value="dx"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="AndroidEnableProfiledAot" type="msb:boolean" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AndroidEnableProfiledAot" _locComment="" -->由 Xamarin.Android 项目使用。一个布尔属性，用于确定在预先编译时是否使用 AOT 配置文件。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="AndroidLinkMode" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AndroidLinkMode" _locComment="" -->由 Xamarin.Android 项目使用。一个字符串属性，指定应为 Android 包中包含的程序集执行哪种类型的链接。仅在 Android 应用程序项目中使用。</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="None"/>
          <xs:enumeration value="SdkOnly"/>
          <xs:enumeration value="Full"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="AndroidLinkTool" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AndroidLinkTool" _locComment="" -->由 Xamarin.Android 项目使用。一个字符串属性，指示要用于 Java 代码的代码收缩器。</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="r8"/>
          <xs:enumeration value="proguard"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="AndroidLinkSkip" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AndroidLinkSkip" _locComment="" -->由 Xamarin.Android 项目使用。指定不应链接的程序集的名称(不含文件扩展名)的以分号(; )分隔的列表。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="AndroidPackageFormat" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AndroidPackageFormat" _locComment="" -->由 Xamarin.Android 项目使用。一个字符串属性，用于指示要将 Android 应用程序打包为 APK 文件还是 Android 应用程序包。</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="apk"/>
          <xs:enumeration value="aab"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="AndroidSupportedAbis" type="msb:StringPropertyType" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AndroidSupportedAbis" _locComment="" -->由 Xamarin.Android 项目使用。一个字符串属性，包含应含入应用程序中的 ABI 的列表，并由分号(;)分隔。</xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name="AotAssemblies" type="msb:boolean" substitutionGroup="msb:Property">
      <xs:annotation>
        <xs:documentation><!-- _locID_text="AotAssemblies" _locComment="" -->由 Xamarin.Android 项目使用。一个布尔属性，用于确定是否将程序集预先编译为本机代码。</xs:documentation>
      </xs:annotation>
    </xs:element>

    <!-- ======================== TASKS =====================================-->
    <xs:element name="AL" substitutionGroup="msb:Task">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="msb:TaskType">
                    <xs:attribute name="AlgorithmId"/>
                    <xs:attribute name="BaseAddress"/>
                    <xs:attribute name="CompanyName"/>
                    <xs:attribute name="Configuration"/>
                    <xs:attribute name="Copyright"/>
                    <xs:attribute name="Culture"/>
                    <xs:attribute name="DelaySign" type="msb:boolean"/>
                    <xs:attribute name="Description"/>
                    <xs:attribute name="EmbedResources"/>
                    <xs:attribute name="EnvironmentVariables"/>
                    <xs:attribute name="EvidenceFile"/>
                    <xs:attribute name="FileVersion"/>
                    <xs:attribute name="Flags"/>
                    <xs:attribute name="GenerateFullPaths" type="msb:boolean"/>
                    <xs:attribute name="KeyContainer"/>
                    <xs:attribute name="KeyFile"/>
                    <xs:attribute name="LinkResources"/>
                    <xs:attribute name="LogStandardErrorAsError" type="msb:boolean"/>
                    <xs:attribute name="MainEntryPoint"/>
                    <xs:attribute name="OutputAssembly" use="required"/>
                    <xs:attribute name="Platform"/>
                    <xs:attribute name="ProductName"/>
                    <xs:attribute name="ProductVersion"/>
                    <xs:attribute name="ResponseFiles"/>
                    <xs:attribute name="SdkToolsPath"/>
                    <xs:attribute name="SourceModules"/>
                    <xs:attribute name="StandardErrorImportance"/>
                    <xs:attribute name="StandardOutputImportance"/>
                    <xs:attribute name="TargetType"/>
                    <xs:attribute name="TemplateFile"/>
                    <xs:attribute name="Timeout"/>
                    <xs:attribute name="Title"/>
                    <xs:attribute name="ToolExe"/>
                    <xs:attribute name="ToolPath"/>
                    <xs:attribute name="Trademark"/>
                    <xs:attribute name="Version"/>
                    <xs:attribute name="Win32Icon"/>
                    <xs:attribute name="Win32Resource"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="AspNetCompiler" substitutionGroup="msb:Task">
        <xs:complexType>
