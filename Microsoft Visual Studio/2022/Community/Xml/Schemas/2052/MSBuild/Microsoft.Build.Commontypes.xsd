                    <!-- _locID_text="PackageReference_IncludeAssets" _locComment="" -->此引用中要包含的资产
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ExcludeAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_ExcludeAssets" _locComment="" -->此引用中要排除的资产
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PrivateAssets">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_PrivateAssets" _locComment="" -->此引用中专用的资产
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="NoWarn">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_NoWarn" _locComment="" -->要忽略的警告代码的分号分隔列表(如 NU1605)
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="GeneratePathProperty">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_GeneratePathProperty" _locComment="" -->设置为 true 以生成指向 NuGet 包内容的还原位置的 Pkg* 属性。如果未设置，请不要创建属性。
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="VersionOverride">
                <xs:annotation>
                  <xs:documentation>
                    <!-- _locID_text="PackageReference_VersionOverride" _locComment="" -->使用中央包管理 (CPM) 时，将替代此包的中央定义版本。如果项目未使用 CPM，则此元素将不起作用。
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
          </xs:sequence>
          <xs:attribute name="Include" type="xs:string">
