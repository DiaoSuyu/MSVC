          </xs:attribute>
          <xs:attribute name="Version" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Version" _locComment="" -->依赖项版本
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="IncludeAssets" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_IncludeAssets" _locComment="" -->此引用中要包含的资产
                                </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ExcludeAssets" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_ExcludeAssets" _locComment="" -->此引用中要排除的资产
                  </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="PrivateAssets" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_PrivateAssets" _locComment="" -->此引用中专用的资产
                                </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="NoWarn" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_NoWarn" _locComment="" -->要忽略的警告代码的分号分隔列表(如 NU1605)
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="GeneratePathProperty" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_GeneratePathProperty" _locComment="" -->设置为 true 以生成指向 NuGet 包内容的还原位置的 Pkg* 属性
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="VersionOverride" type="xs:string" use="optional">
            <xs:annotation>
              <xs:documentation>
                <!-- _locID_text="PackageReference_Attribute_VersionOverride" _locComment="" -->使用中央包管理 (CPM) 时，将替代此包的中央定义版本。如果项目未使用 CPM，则此属性将不起作用。
              </xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
