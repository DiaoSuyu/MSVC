<?xml version="1.0" encoding="utf-8"?>
<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>
      <!-- _locID_text="title" _locComment="" -->Snippet</Title>
    <Author>Microsoft Corporation</Author>
    <Shortcut>snippet</Shortcut>
    <Description>
      <!-- _locID_text="description" _locComment="" -->用于创建新的 XML 片段的 XML 片段!</Description>
    <SnippetTypes>
      <SnippetType>SurroundsWith</SnippetType>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>title</ID>
        <Default><!-- _locID_text="snippettitle" _locComment="" -->标题</Default>
      </Literal>
      <Literal>
        <ID>author</ID>
        <Default><!-- _locID_text="snippetauthor" _locComment="" -->作者</Default>
      </Literal>
      <Literal>
        <ID>shortcut</ID>
        <Default><!-- _locID_text="shortcut" _locComment="" -->快捷方式</Default>
      </Literal>
      <Literal>
        <ID>description</ID>
        <Default><!-- _locID_text="snippetdescription" _locComment="" -->说明</Default>
      </Literal>
      <Literal>
        <ID>name</ID>
        <Default><!-- _locID_text="name" _locComment="" -->名称</Default>
      </Literal>
      <Literal>
        <ID>value</ID>
        <Default><!-- _locID_text="value" _locComment="" -->值</Default>
      </Literal>
      <Literal Editable="false">
        <ID>endcdata</ID>
        <Default>]]&gt;</Default>
      </Literal>

    </Declarations>
    <Code Language="XML"><![CDATA[<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>$title$</Title>
    <Author>$author$</Author>
    <Shortcut>$shortcut$</Shortcut>
    <Description>$description$</Description>
    <SnippetTypes>
      <SnippetType>SurroundsWith</SnippetType>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
    <Literal>
        <ID>$name$</ID>
        <Default>$value$</Default>
    </Literal>
    </Declarations>
    <Code Language="XML"><![CDATA[$end$<test>
      <name>$$$name$$$</name>
      $selected$$$selected$$ $$end$$</test>$endcdata$
    </Code>
    </Snippet>
</CodeSnippet>]]>
    </Code>
  </Snippet>
</CodeSnippet>
