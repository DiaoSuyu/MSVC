<?xml version="1.0" encoding="utf-8"?>
<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>
      <!-- _locID_text="title" _locComment="" -->sequence</Title>
    <Author>Microsoft Corporation</Author>
    <Shortcut>elsequence</Shortcut>
    <Description>
      <!-- _locID_text="description" _locComment="" -->用于带有 XSD sequence 的 XSD complexType 元素的 XML 片段</Description>
    <SnippetTypes>
      <SnippetType>SurroundsWith</SnippetType>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>name</ID>
        <ToolTip>
          <!-- _locID_text="tooltip" _locComment="" -->元素名称。</ToolTip>
        <Default>name</Default>
      </Literal>
    </Declarations>
    <Code Language="XML"><![CDATA[<xsd:element name="$name$" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:complexType>
    <xsd:sequence>$selected$$end$</xsd:sequence>
    </xsd:complexType>
    </xsd:element>]]></Code>
  </Snippet>
</CodeSnippet>
