<?xml version="1.0" encoding="utf-8"?>
<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>
      <!-- _locID_text="title" _locComment="" -->documentation</Title>
    <Author>Microsoft Corporation</Author>
    <Shortcut>doc</Shortcut>
    <Description>
      <!-- _locID_text="description" _locComment="" -->用于 XSD 文档的 XML 片段</Description>
    <SnippetTypes>
      <SnippetType>SurroundsWith</SnippetType>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>text</ID>
        <Default>
          <!-- _locID_text="default" _locComment="" -->文本</Default>
        <ToolTip>
          <!-- _locID_text="tooltip" _locComment="" -->输入 XSD 文档的文本
        </ToolTip>
      </Literal>
    </Declarations>
    <Code Language="XML"><![CDATA[<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema"><xsd:documentation>$text$$selected$$end$</xsd:documentation></xsd:annotation>]]></Code>
  </Snippet>
</CodeSnippet>
