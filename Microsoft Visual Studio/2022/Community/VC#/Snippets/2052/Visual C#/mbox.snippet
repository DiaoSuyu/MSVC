<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>mbox</Title>
			<Shortcut>mbox</Shortcut>
			<Description>MessageBox.Show 的代码片段</Description>
			<Author>Microsoft Corporation</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>string</ID>
					<ToolTip>要显示的字符串</ToolTip>
					<Default>"Test"</Default>
				</Literal>
				<Literal Editable="false">
					<ID>SystemWindowsFormsMessageBox</ID>
					<Function>SimpleTypeName(global::System.Windows.Forms.MessageBox)</Function>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[$SystemWindowsFormsMessageBox$.Show($string$);$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
