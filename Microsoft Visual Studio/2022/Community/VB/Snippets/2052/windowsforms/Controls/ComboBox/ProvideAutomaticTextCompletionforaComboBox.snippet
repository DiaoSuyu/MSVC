<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>为 ComboBox 提供文本自动完成</Title>
      <Author>Microsoft Corporation</Author>
      <Description>为 ComboBox 提供文本自动完成。</Description>
      <Shortcut>cbComplete</Shortcut>
    </Header>
    <Snippet>
		<References>
			<Reference>
				<Assembly>System.Windows.Forms</Assembly>
			</Reference>
		</References>
		<Imports>
			<Import>
				<Namespace>Microsoft.VisualBasic</Namespace>
			</Import>
			<Import>
				<Namespace>System.Windows.Forms</Namespace>
			</Import>
		</Imports>
      <Declarations>
        <Object>
          <ID>comboBoxName</ID>
          <Type>ComboBox</Type>
          <ToolTip>替换为窗体上的 ComboxBox。</ToolTip>
          <Default>ComboBox1</Default>
        </Object>
        <Literal>
          <ID>mode</ID>
          <ToolTip>替换为所需的自动完成模式(Append、None、Suggest 或 SuggestAttend)。</ToolTip>
          <Default>Suggest</Default>
        </Literal>
        <Literal>
          <ID>source</ID>
          <ToolTip>替换为要用于自动完成的源。</ToolTip>
          <Default>AllUrl</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method body"><![CDATA[$comboBoxName$.AutoCompleteMode = AutoCompleteMode.$mode$
$comboBoxName$.AutoCompleteSource = AutoCompleteSource.$source$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
