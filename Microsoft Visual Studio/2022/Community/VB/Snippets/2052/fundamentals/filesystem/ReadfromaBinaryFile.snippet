<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>读取二进制文件</Title>
      <Author>Microsoft Corporation</Author>
      <Description>从二进制文件中读取数据。</Description>
      <Shortcut>filReadBin</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>Filename</ID>
          <Type>String</Type>
          <ToolTip>替换为文件名。</ToolTip>
          <Default>"C:\Test.txt"</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method body"><![CDATA[Dim fileContents As Byte()
filecontents = My.Computer.FileSystem.ReadAllBytes($Filename$)]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
