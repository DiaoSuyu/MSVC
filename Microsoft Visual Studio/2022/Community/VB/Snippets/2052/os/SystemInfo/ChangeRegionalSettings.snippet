<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>更改区域设置</Title>
      <Author>Microsoft Corporation</Author>
      <Description>更改区域设置。</Description>
      <Shortcut>sysChgSet</Shortcut>
    </Header>
    <Snippet>
      <References>
        <Reference>
          <Assembly>System.dll</Assembly>
        </Reference>
      </References>
      <Imports>
        <Import>
          <Namespace>Microsoft.VisualBasic</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal>
          <ID>CultureString</ID>
          <Type>String</Type>
          <ToolTip>替换为区域性字符串。</ToolTip>
          <Default>"en-us"</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method body"><![CDATA[My.Application.ChangeCulture($CultureString$)
My.Application.ChangeUICulture($CultureString$)
]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
