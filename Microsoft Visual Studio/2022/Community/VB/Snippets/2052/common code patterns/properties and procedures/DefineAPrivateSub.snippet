<?xml version="1.0" encoding="UTF-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>定义私有的 Sub</Title>
      <Author>Microsoft Corporation</Author>
      <Description>使用 Sub 关键字定义不返回值的私有方法。</Description>
      <Keywords>
      </Keywords>
      <Shortcut>SubPriv</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>MethodName</ID>
          <Type>String</Type>
          <ToolTip>替换为方法名称。</ToolTip>
          <Default>MyMethod</Default>
        </Literal>
      </Declarations>      
      <Code Language="VB" Kind="method decl"><![CDATA[Private Sub $MethodName$()

End Sub]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
