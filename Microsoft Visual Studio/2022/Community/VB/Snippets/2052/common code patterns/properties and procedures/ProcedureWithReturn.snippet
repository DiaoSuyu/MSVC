<?xml version="1.0" encoding="UTF-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>定义函数</Title>
      <Author>Microsoft Corporation</Author>
      <Description>使用 Function 关键字定义返回值的过程。</Description>
      <Keywords>
      </Keywords>
      <Shortcut>Function</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>FuncName</ID>
          <Type></Type>
          <ToolTip>替换为函数的名称。</ToolTip>
          <Default>MyFunc</Default>
        </Literal>
        <Literal>
          <ID>ReturnType</ID>
          <Type></Type>
          <ToolTip>替换为函数的返回类型。</ToolTip>
          <Default>Integer</Default>
        </Literal>
        <Literal>
          <ID>ReturnValue</ID>
          <Type></Type>
          <ToolTip>替换为函数的返回值。</ToolTip>
          <Default>0</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method decl"><![CDATA[Function $FuncName$() As $ReturnType$

Return $ReturnValue$
End Function	]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
