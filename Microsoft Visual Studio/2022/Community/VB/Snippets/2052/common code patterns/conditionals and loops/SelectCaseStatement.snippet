<?xml version="1.0" encoding="UTF-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>选择 Case 语句</Title>
      <Author>Microsoft Corporation</Author>
      <Description>插入 Select Case 语句。</Description>
      <Shortcut>Select</Shortcut>
    </Header>
    <Snippet>
      <Imports>
      </Imports>
      <Declarations>
        <Object>
          <ID>Variable</ID>
          <Type>Object</Type>
          <ToolTip>替换为表达式。</ToolTip>
          <Default>variableName</Default>
        </Object>
        <Literal>
          <ID>Case1</ID>
          <Type></Type>
          <ToolTip>替换为表达式的有效值。</ToolTip>
          <Default>1</Default>
        </Literal>
        <Literal>
          <ID>Case2</ID>
          <Type></Type>
          <ToolTip>替换为表达式的其他有效值。</ToolTip>
          <Default>2</Default>
        </Literal>
      </Declarations>
      <Code Language="VB" Kind="method body"><![CDATA[Select Case $Variable$
    Case $Case1$

    Case $Case2$

    Case Else

End Select]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
